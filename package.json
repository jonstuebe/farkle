{
  "name": "farkle",
  "version": "1.0.0",
  "description": "A small utility to keep track of your score in Farkle.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.js",
    "build": "npm run clean && cp -r src/index.html ./index.html && npm run build:webpack",
    "start": "npm run clean && cp -r src/dev.html ./index.html && node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonstuebe/farkle.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Jon Stuebe <jstuebe@gmail.com> (http://github.com/jonstuebe)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonstuebe/farkle/issues"
  },
  "homepage": "https://github.com/jonstuebe/farkle",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.7.0",
    "npm-install-webpack-plugin": "^4.0.4",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "firebase": "^3.0.4",
    "history": "^1.17.0",
    "immutable": "^3.8.1",
    "react": "^0.14.8",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.3",
    "reactfire": "^1.0.0",
    "reflux": "^0.4.1"
  }
